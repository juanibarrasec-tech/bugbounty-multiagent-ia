# .env.example template configuration

# OpenAI API key
OPENAI_API_KEY=your_openai_api_key_here

# Claude API key
CLAUDE_API_KEY=your_claude_api_key_here

# Telegram token and chat ID
TELEGRAM_TOKEN=your_telegram_token_here
TELEGRAM_CHAT_ID=your_chat_id_here

# Discord webhook
DISCORD_WEBHOOK=your_discord_webhook_here

# SMTP email settings
SMTP_HOST=your_smtp_host_here
SMTP_PORT=your_smtp_port_here
SMTP_USER=your_email_here
SMTP_PASSWORD=your_email_password_here

# HackerOne credentials
HACKERONE_USERNAME=your_hackerone_username_here
HACKERONE_PASSWORD=your_hackerone_password_here

# Bugcrowd token
BUGCROWD_TOKEN=your_bugcrowd_token_here

# Local configuration parameters
LOCAL_PARAM_1=your_local_param_value_1_here
LOCAL_PARAM_2=your_local_param_value_2_here